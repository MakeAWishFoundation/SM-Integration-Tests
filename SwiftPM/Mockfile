# Mockfile is a SwiftyMocky YAML configuration file
sourceryCommand: sourcery

unit.prototype:
  prototype: true
  sources:
    include:
        - ./Sources/Unit
    exclude: []
  output:
    ./Sources/Unit/Prototypes.generated.swift
  targets:
    - Package.swift/Unit
  import:
    - Foundation

unit.tests.mock:
  sources:
    include:
        - ./Sources/Unit
    exclude: []
  output:
    ./Tests/UnitTests/Mock.generated.swift
  targets:
    - Package.swift/UnitTests
  testable:
    - Unit
  import:
    - Foundation
